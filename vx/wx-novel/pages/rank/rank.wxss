.rank {
  height: 100vh;
  overflow: hidden;
}
.rank .left {
  width: 18%;
  height: 500px;
  float: left;
  padding: 0 14px;
  font-size: 14px;
  overflow: scroll;
}
.rank .left .title {
  line-height: 4;
  text-align: center;
}
.rank .right {
  width: 70%;
  height: 500px;
  float: left;
  padding-right: 10px;
  overflow: scroll;
}
